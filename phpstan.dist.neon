includes:
    - phpstan-baseline.neon

parameters:
    level: max
    checkUninitializedProperties: true
    paths:
        - config/
        - src/
        - tests/
    excludePaths:
        - src/Infrastructure/Migration/*
    ignoreErrors:
        -
            message: '#^Property [a-zA-Z0-9\\_]+::\$[a-zA-Z\_]+ is never read, only written.$#'
